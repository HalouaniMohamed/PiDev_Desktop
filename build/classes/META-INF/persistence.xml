<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="DesktopPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>pidev_desktop.entities.JournalMood</class>
    <class>pidev_desktop.entities.Post</class>
    <class>pidev_desktop.entities.Mood</class>
    <class>pidev_desktop.entities.RendezVous</class>
    <class>pidev_desktop.entities.Article</class>
    <class>pidev_desktop.entities.Forum</class>
    <class>pidev_desktop.entities.Commentaire</class>
    <class>pidev_desktop.entities.MessengerMessages</class>
    <class>pidev_desktop.entities.Notification</class>
    <class>pidev_desktop.entities.Cabinet</class>
    <class>pidev_desktop.entities.Medecin</class>
    <class>pidev_desktop.entities.DoctrineMigrationVersions</class>
    <class>pidev_desktop.entities.Evenements</class>
    <class>pidev_desktop.entities.Reservation</class>
    <class>entities.ShoppingCartItem</class>
    <class>entities.User</class>
    <class>entities.Category</class>
    <class>entities.Commande</class>
    <class>entities.Product</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/pidevfinal?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
